    /** ************************************************************************
     * 
     * @return 
     */
    public String getPrijmeniAutora() {
        String value = getAutor();
        StringBuilder exit = new StringBuilder();

        if (value == null) return null;

        for (int i = 0; i < value.length(); i++) {
            if (Character.isAlphabetic(value.charAt(i)) || Character.isDigit(value.charAt(i))) {
                exit.append(value.charAt(i));
            } else if (exit.length() != 0) {
                LOG.log(Level.INFO, "Prijmeni autora: {0}", exit.toString());
                return exit.toString();
            }
        }

        LOG.log(Level.INFO, "Prijmeni autora: {0}", exit.toString());
        return exit.toString();
    }

    /** ************************************************************************
     * 
     * @return 
     */
    public String getNazevDoDvojteckyNeboLomitka() {
        String value = getNazev();
        if (value == null) return null;
        StringBuilder exit = new StringBuilder();

        value = value.trim();
        for (int i = 0; i < value.length(); i++) {
            if (value.charAt(i) == 47 || value.charAt(i) == 58) {
                LOG.log(Level.INFO, "Nazev do dvojtecky nebo lomitka: {0}", exit.toString());
                return exit.toString();
            } else {
                exit.append(value.charAt(i));
            }
        }

        LOG.log(Level.INFO, "Nazev do dvojtecky nebo lomitka: {0}", exit.toString());
        return exit.toString();
    }

